{
  "name": "@lens-protocol/gated-content",
  "version": "0.0.1",
  "description": "Logic around token gated content",
  "main": "dist/lens-protocol-gated-content.cjs.js",
  "module": "dist/lens-protocol-gated-content.esm.js",
  "exports": {
    ".": {
      "module": "./dist/lens-protocol-gated-content.esm.js",
      "default": "./dist/lens-protocol-gated-content.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "directory": "packages/gated-content",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "preconstruct build",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@lens-protocol/shared-kernel": "workspace:*",
    "@lit-protocol/lit-node-client": "^2.1.38",
    "ethers": "^5.7.2",
    "siwe": "^1.1.6",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.4.3",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@types/jest": "29.2.3",
    "@types/jest-when": "^3.5.2",
    "eslint": "^8.34.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-when": "^3.5.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "wait-for-expect": "^3.0.2"
  },
  "peerDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-when": "^3.5.2"
  },
  "prettier": "@lens-protocol/prettier-config",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ],
    "exports": true
  }
}
